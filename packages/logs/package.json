{
  "name": "@agent-toolkit/logs",
  "version": "0.1.0",
  "description": "Log reading toolkit for Cloud Run logs using Google Cloud Logging SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./claude-agent-sdk": {
      "types": "./dist/sdks/claude-agent-sdk/index.d.ts",
      "default": "./dist/sdks/claude-agent-sdk/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "logs",
    "log-reading",
    "cloud-run",
    "gcloud",
    "google-cloud-logging",
    "agent",
    "toolkit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjulin/agent-toolkit.git",
    "directory": "packages/logs"
  },
  "bugs": {
    "url": "https://github.com/kjulin/agent-toolkit/issues"
  },
  "homepage": "https://github.com/kjulin/agent-toolkit/tree/main/packages/logs#readme",
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@google-cloud/logging": "^11.0.0"
  },
  "peerDependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.30",
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/claude-agent-sdk": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.30",
    "@types/node": "^20.19.24",
    "typescript": "^5.9.0",
    "vitest": "^2.0.0",
    "zod": "^3.25.76"
  }
}
